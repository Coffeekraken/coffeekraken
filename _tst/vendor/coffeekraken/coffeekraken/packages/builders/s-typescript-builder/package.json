{
    "name": "@coffeekraken/s-typescript-builder",
    "version": "2.0.0-alpha.24",
    "description": "Typescript builder that let you transpile your .ts files with ease. It allows to generate multiple formats (esm,cjs) at once if needed.",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-builder": "^2.0.0-alpha.24",
        "@coffeekraken/s-class": "^2.0.0-alpha.24",
        "@coffeekraken/s-env": "^2.0.0-alpha.24",
        "@coffeekraken/s-file": "^2.0.0-alpha.24",
        "@coffeekraken/s-glob": "^2.0.0-alpha.24",
        "@coffeekraken/s-interface": "^2.0.0-alpha.24",
        "@coffeekraken/s-log": "^2.0.0-alpha.24",
        "@coffeekraken/s-monorepo": "^2.0.0-alpha.24",
        "@coffeekraken/s-promise": "^2.0.0-alpha.24",
        "@coffeekraken/s-sugar-config": "^2.0.0-alpha.24",
        "@coffeekraken/sugar": "^2.0.0-alpha.24",
        "@ts-morph/bootstrap": "^0.18.0",
        "chokidar": "^3.5.3",
        "ts-morph": "^17.0.1",
        "typescript": "^4.7.4"
    },
    "main": "dist/pkg/cjs/node/exports.js",
    "module": "dist/pkg/esm/node/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/node/exports.js",
            "import": "./dist/pkg/esm/node/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}
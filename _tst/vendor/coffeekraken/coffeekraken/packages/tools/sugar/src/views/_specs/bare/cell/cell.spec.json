{
    "type": "Bare",
    "title": "Cell",
    "description": "Simple cell that can be used inside a layout, etc...",
    "props": {
        "attributes": "@sugar.views.props.attributes",
        "html": {
            "type": "Html",
            "description": "The html to print in the cell",
            "required": true
        },
        "margin": "@sugar.views.props.margin",
        "padding": "@sugar.views.props.padding"
    },
    "examples": {
        "twig": {
            "title": "Twig",
            "description": "Here's a twig sample of how to use the cell view",
            "code": [
                "{% include 'sugar.bare.cell.cell' with {",
                "   html: '<h1>Hello world</h1>',",
                "} %}"
            ]
        }
    }
}

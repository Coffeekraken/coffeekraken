{
  "name": "@coffeekraken/starter-kit",
  "version": "1.0.0",
  "description": "Base HTML files and folder structure with complete build process (js, scss, image compression, etc...) built in",
  "homepage": "https://coffeekraken.io",
  "scripts": {
    "dist": "npm run dist.js && npm run dist.scss && npm run dist.img && npm run dist.fonts",
    "dist.js": "rm -rf dist/js && webpack",
    "dist.js.analyzer": "rm -rf dist/js && webpack --config webpack.analyzer.config.js",
    "dist.scss": "sass src/scss/style.scss dist/css/style.css",
    "dist.img": "coffeekraken-imagemin -s src/img -o dist/img -q 80",
    "dist.fonts": "rm -rf dist/fonts && cp -R src/fonts dist/fonts",
    "pretty-quick": "pretty-quick",
    "open-browser": "open http://0.0.0.0:8080",
    "server": "php -S 0.0.0.0:8080 -t .",
    "start": "coffeekraken-scripts-stack server open-browser -i start -i dist"
  },
  "watch": {
    "dist.js": {
      "paths": "src/js/**/*.js"
    },
    "dist.scss": {
      "paths": "src/scss/**/*.scss"
    },
    "dist.img": {
      "paths": "src/img/**/*"
    },
    "dist.fonts": {
      "paths": "src/fonts/**/*"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "coffee",
    "kraken",
    "coffeekraken",
    "starter",
    "kit",
    "sass",
    "css",
    "html",
    "js",
    "npm"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "contributors": [
    {
      "name": "Olivier Bossel",
      "email": "olivier.bossel@gmail.com",
      "url": "http://olivierbossel.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@coffeekraken/imagemin": "^1.0.0",
    "@coffeekraken/scripts-stack": "^0.0.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "http-server": "^0.11.1",
    "husky": "^3.0.9",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "sass": "^1.23.3",
    "stylelint": "^12.0.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@coffeekraken/gridle": "0.0.4",
    "core-js": "^3.4.1",
    "lodash": "^4.17.15"
  }
}

{
  "name": "coffeekraken",
  "version": "1.0.0",
  "description": "We are a young collectve of front-end creative developers with one goal in mind. Build tools to make every team working day life better",
  "scripts": {
    "start": "node ./toolkits/sugar/src/cli/sugar.cli.js bootstrap.monorepo",
    "dev": "npm run dev.js; npm run dev.node",
    "dev.js": "tsc -p tsconfig.js.json --watch",
    "dev.node": "tsc -p tsconfig.node.json --watch",
    "lint": "eslint 'packages/**/*.{js,ts,tsx}' --quiet --fix",
    "container": "docker exec -it coffeekraken /bin/bash",
    "init-sugar": "(cd toolkits/sugar && npm link && npm i)"
  },
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coffeekraken/coffeekraken.git"
  },
  "keywords": [
    "coffee",
    "kraken",
    "coffeekraken"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "contributors": [
    {
      "name": "Olivier Bossel",
      "email": "olivier.bossel@gmail.com",
      "url": "http://olivierbossel.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Coffeekraken/coffeekraken/issues"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "none"
  },
  "homepage": "https://github.com/Coffeekraken/coffeekraken#readme",
  "dependencies": {
    "@coffeekraken/s-filtrable-input": "file:webcomponents/s-filtrable-input",
    "@coffeekraken/sugar": "file:toolkits/sugar"
  },
  "globalDependencies": {},
  "devDependencies": {
    "@shopify/jest-dom-mocks": "^2.8.11",
    "@testing-library/jest-dom": "^5.3.0",
    "@types/node": "^14.14.8",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "^7.14.0",
    "find-in-files": "^0.5.0",
    "jest": "^25.2.7",
    "jest-localstorage-mock": "^2.4.0",
    "jest-puppeteer": "^4.4.0",
    "jest-useragent-mock": "^0.1.1",
    "local-storage": "^2.0.0",
    "mutation-observer": "^1.0.3",
    "mutationobserver-shim": "^0.3.5",
    "node-persist": "^3.0.5",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.1",
    "pretty-error": "^2.1.1",
    "puppeteer": "^2.1.1",
    "replace-in-files": "^2.0.3",
    "rimraf": "^3.0.2",
    "semistandard": "^16.0.0",
    "symlink-dir": "^3.1.1",
    "typescript": "^4.1.2",
    "webpack-cli": "^3.3.10"
  }
}

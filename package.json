{
  "name": "coffeekraken",
  "version": "1.0.0",
  "description": "We are a young collectve of front-end creative developers with one goal in mind. Build tools to make every team working day life better",
  "scripts": {
    "dev": "tsc -p .tsconfig/tsconfig.all.json --watch",
    "dev.js": "tsc -p .tsconfig/tsconfig.js.json --watch",
    "dev.node": "tsc -p .tsconfig/tsconfig.node.json --watch",
    "dev.shared": "tsc -p .tsconfig/tsconfig.shared.json --watch",
    "container": "docker exec --workdir /workspaces/coffeekraken -it coffeekraken /bin/bash"
  },
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coffeekraken/coffeekraken.git"
  },
  "keywords": [
    "coffee",
    "kraken",
    "coffeekraken"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "contributors": [
    {
      "name": "Olivier Bossel",
      "email": "olivier.bossel@gmail.com",
      "url": "http://olivierbossel.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Coffeekraken/coffeekraken/issues"
  },
  "homepage": "https://github.com/Coffeekraken/coffeekraken#readme",
  "dependencies": {
    "dependency-check": "^4.1.0",
    "esm": "^3.2.25",
    "jest-esm-transformer": "^1.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "ts-jest": "^27.0.3",
    "typescript-esm": "^2.0.0"
  },
  "globalDependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "jest": "^27.0.6",
    "typescript": "^4.3.5"
  }
}

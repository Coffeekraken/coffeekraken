{
  "name": "coffeekraken",
  "version": "1.0.0",
  "description": "We are a young collectve of front-end creative developers with one goal in mind. Build tools to make every team working day life better",
  "scripts": {
    "start": "npm run install-lerna && lerna bootstrap && npx lerna link && npm run init-sugar",
    "install-lerna": "(npm list -g | grep lerna > /dev/null 2>&1) || npm install -g lerna",
    "init-sugar": "(cd toolkits/sugar && npm link && npm i)"
  },
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coffeekraken/coffeekraken.git"
  },
  "keywords": [
    "coffee",
    "kraken",
    "coffeekraken"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "contributors": [
    {
      "name": "Olivier Bossel",
      "email": "olivier.bossel@gmail.com",
      "url": "http://olivierbossel.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Coffeekraken/coffeekraken/issues"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "none"
  },
  "homepage": "https://github.com/Coffeekraken/coffeekraken#readme",
  "_moduleAliases": {
    "@coffeekraken/webpack-coffee-loader": "webpack/coffee-loader",
    "@coffeekraken/coffeebuilder": "terminal/coffeebuilder",
    "@coffeekraken/webpack-concat-dependencies-vendors-plugin": "webpack/concat-dependencies-vendors-plugin",
    "@coffeekraken/webpack-lazy-dom-load-plugin": "webpack/lazy-dom-load-plugin",
    "@coffeekraken/sugar/js": "util/sugar/dist/js",
    "@coffeekraken/sugar/node": "util/sugar/src/node"
  },
  "dependencies": {
    "@coffeekraken/s-filtrable-input": "file:webcomponents/s-filtrable-input",
    "@coffeekraken/sugar": "file:toolkits/sugar"
  },
  "globalDependencies": {},
  "devDependencies": {
    "@shopify/jest-dom-mocks": "^2.8.11",
    "@testing-library/jest-dom": "^5.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "find-in-files": "^0.5.0",
    "jest": "^25.2.7",
    "jest-localstorage-mock": "^2.4.0",
    "jest-puppeteer": "^4.4.0",
    "jest-useragent-mock": "^0.1.1",
    "lerna": "^3.22.1",
    "lerna-wizard": "^1.1.1",
    "local-storage": "^2.0.0",
    "mutation-observer": "^1.0.3",
    "mutationobserver-shim": "^0.3.5",
    "node-persist": "^3.0.5",
    "node-sass": "^4.14.1",
    "pretty-error": "^2.1.1",
    "puppeteer": "^2.1.1",
    "replace-in-files": "^2.0.3",
    "rimraf": "^3.0.2",
    "symlink-dir": "^3.1.1",
    "webpack-cli": "^3.3.10"
  }
}

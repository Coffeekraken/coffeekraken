{
    "$custom": {
        "menus": {
            "primary": {
                "name": "Primary menu"
            },
            "footer": {
                "name": "Footer menu"
            }
        }
    },
    "metas": {
        "lang": "en",
        "title": "01-basic",
        "homepage": "",
        "description": "",
        "themeColor": "#ffbb00",
        "author": {
            "name": "",
            "email": "",
            "url": ""
        },
        "og": {
            "title": "01-basic",
            "description": "",
            "type": "website",
            "url": "",
            "image": "https://cdnv2.coffeekraken.io/coffeekraken-og.png"
        }
    },
    "assets": {
        "dev": {
            "type": "module",
            "defer": true,
            "src": "/src/js/index.ts",
            "env": "development"
        },
        "module": {
            "type": "module",
            "defer": true,
            "src": "/dist/js/index.esm.js",
            "env": "production"
        },
        "nomodule": {
            "nomodule": true,
            "defer": true,
            "src": "/dist/js/index.amd.js",
            "env": "production"
        },
        "style": {
            "id": "global",
            "defer": true,
            "src": "/dist/css/index.css"
        }
    },
    "media": {
        "defaultAction": "<=",
        "defaultQuery": "screen",
        "queries": {
            "mobile": {
                "min-width": 0,
                "max-width": 639
            },
            "tablet": {
                "min-width": 640,
                "max-width": 1279
            },
            "desktop": {
                "min-width": 1280,
                "max-width": 2047
            },
            "wide": {
                "min-width": 2048,
                "max-width": null
            },
            "dwarf": {
                "min-height": null,
                "max-height": 700
            }
        }
    },
    "views": {
        "rootDirs": {
            "twig": [
                "./src/views",
                "./node_modules/@coffeekraken/sugar/src/views/twig"
            ],
            "blade": [
                "./src/views",
                "./node_modules/@coffeekraken/sugar/src/views/blade"
            ]
        }
    },
    "specs": {
        "namespaces": {
            "sugar.views": [
                "./node_modules/@coffeekraken/sugar/src/views"
            ],
            "views": [
                "./src/views"
            ],
            "bare": [
                "./src/views/bare"
            ],
            "sections": [
                "./src/views/sections"
            ],
            "components": [
                "./src/views/components"
            ]
        }
    },
    "google": {},

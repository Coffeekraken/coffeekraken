{
    "name": "@coffeekraken/s-dashboard",
    "version": "2.0.0-beta.1",
    "description": "Enable a nice dashboard on your website that let you check if things like core vitals, best practices, etc... are correct",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "scripts": {
        "build": "node --experimental-json-modules --no-warnings --es-module-specifier-resolution node src/node/build.js",
        "deploy": "rsync -avzh --delete --progress --stats ./dist/ fe0mt_coffeekraken@fe0mt.ftp.infomaniak.com:~/sites/cdnv2.coffeekraken.io/s-dashboard"
    },
    "dependencies": {
        "@coffeekraken/s-class": "^2.0.0-beta.1",
        "@coffeekraken/s-filtrable-input-component": "^2.0.0-beta.1",
        "@coffeekraken/s-frontend-checker": "^2.0.0-beta.1",
        "@coffeekraken/s-interface": "^2.0.0-beta.1",
        "@coffeekraken/s-lit-component": "^2.0.0-beta.1",
        "@coffeekraken/s-promise": "^2.0.0-beta.1",
        "@coffeekraken/s-request": "^2.0.0-beta.1",
        "@coffeekraken/s-sugar-config": "^2.0.0-beta.1",
        "@coffeekraken/s-vite-postcss-plugin": "workspace:^",
        "@coffeekraken/s-vite-sugar-plugin": "workspace:^",
        "@coffeekraken/sugar": "^2.0.0-beta.1",
        "lit": "^2.2.8"
    },
    "main": "dist/pkg/cjs/js/exports.js",
    "module": "dist/pkg/esm/js/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/js/exports.js",
            "import": "./dist/pkg/esm/js/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.1.0",
        "cross-env": "^7.0.3",
        "vite": "^4.3.9"
    }
}
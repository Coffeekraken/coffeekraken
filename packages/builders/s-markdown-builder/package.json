{
    "name": "@coffeekraken/s-markdown-builder",
    "version": "2.0.0-beta.1",
    "description": "Markdown builder that let you use the AMAZING handlebars.js syntax to generate your final markdown files.",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-builder": "^2.0.0-beta.1",
        "@coffeekraken/s-code-formatter": "^2.0.0-beta.1",
        "@coffeekraken/s-data-file-generic": "^2.0.0-beta.1",
        "@coffeekraken/s-docmap": "^2.0.0-beta.1",
        "@coffeekraken/s-env": "^2.0.0-beta.1",
        "@coffeekraken/s-event-emitter": "^2.0.0-beta.1",
        "@coffeekraken/s-file": "^2.0.0-beta.1",
        "@coffeekraken/s-frontspec": "^2.0.0-beta.1",
        "@coffeekraken/s-glob": "^2.0.0-beta.1",
        "@coffeekraken/s-interface": "^2.0.0-beta.1",
        "@coffeekraken/s-log": "^2.0.0-beta.1",
        "@coffeekraken/s-postcss-sugar-plugin": "^2.0.0-beta.1",
        "@coffeekraken/s-promise": "^2.0.0-beta.1",
        "@coffeekraken/s-specs": "^2.0.0-beta.1",
        "@coffeekraken/s-sugar-config": "^2.0.0-beta.1",
        "@coffeekraken/s-theme": "^2.0.0-beta.1",
        "@coffeekraken/s-view-renderer": "^2.0.0-beta.1",
        "@coffeekraken/sugar": "^2.0.0-beta.1",
        "marked": "^4.0.18",
        "prettier": "^2.8.0",
        "pretty-remarkable": "^1.0.0",
        "remark-parse": "^10.0.1",
        "remarkable": "^2.0.1"
    },
    "main": "dist/pkg/cjs/node/exports.js",
    "module": "dist/pkg/esm/node/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/node/exports.js",
            "import": "./dist/pkg/esm/node/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}

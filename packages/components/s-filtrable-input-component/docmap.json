                    {
                        "language": "html",
                        "title": "Simple example",
                        "code": "<template id=\"items\">\n  [{\"title\":\"Hello\",\"value\":\"hello\"},{\"title\":\"world\",\"value\":\"world\"}]\n</template>\n<s-filtrable-input items=\"#items\" label=\"title\" filtrable=\"title\">\n  <input type=\"text\" class=\"s-input\" placeholder=\"Type something...\" />\n</s-filtrable-input>"
                    },
                    {
                        "language": "js",
                        "code": "import { define } from '@coffeekraken/s-filtrable-input-component';\ndefine();"
                    },
                    {
                        "language": "html",
                        "title": "Custom templates and items",
                        "code": "<my-cool-filtrable-input>\n   <input type=\"text\" class=\"s-input\" placeholder=\"Type something...\" />\n</my-cool-filtrable-input>"
                    },
                    {
                        "language": "js",
                        "code": "import { define } from '@coffeekraken/s-filtrable-input-component';\ndefine({\n    items: async () => {\n        // you can get your items however you want\n        // const request = await fetch('...');\n        // const items = await request.json();\n        return [{title: 'Hello', value: 'World'},{title: 'Plop', value:'Yop}];\n    },\n    templates: ({ type, html }) => {\n        switch (type) {\n            case 'item':\n                return html`\n                    <li class=\"_item\">\n                        My title: ${item.title}\n                    </li>\n                `;\n                break;\n            case 'loading':\n                return html`\n                    <li class=\"_loading\">\n                        Loading, please wait...\n                    </li>\n                `;\n                break;\n            case 'empty':\n                return html`\n                    <li class=\"_empty\">\n                        No items found...\n                    </li>\n                `;\n                break;\n        }\n    },\n}, 'my-cool-filtrable-input');"
                    }
                ],

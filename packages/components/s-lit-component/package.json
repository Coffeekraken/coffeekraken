{
    "name": "@coffeekraken/s-lit-component",
    "version": "2.0.0-alpha.20",
    "description": "Base class to create web components using the AMAZING lit-element package under the hood, with more features like mounting your component only at a certain point in time, etc...",
    "type": "module",
    "homepage": "https://coffeekraken.io",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-component-utils": "^2.0.0-alpha.20",
        "@coffeekraken/s-interface": "^2.0.0-alpha.20",
        "@coffeekraken/s-classmap": "^2.0.0-alpha.20",
        "@coffeekraken/sugar": "^2.0.0-alpha.20",
        "fastdom": "^1.0.11",
        "lit": "^2.2.8"
    },
    "main": "dist/pkg/cjs/js/exports.js",
    "module": "dist/pkg/esm/js/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/js/exports.js",
            "import": "./dist/pkg/esm/js/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    }
}
{
  "config": {
    "folders": [
      {
        "path": "./dist/pkg/%moduleSystem/config"
      }
    ]
  },
  "cli": [
    {
      "stack": "config",
      "description": "Configuration related actions",
      "defaultAction": "cache",
      "actions": {
        "cache": {
          "scope": "global",
          "description": "Put the configuration in the cache to speed up subsequent calls",
          "interface": "./dist/pkg/esm/cli/interface/SCliConfigCacheParamsInterface.js",
          "process": "./dist/pkg/esm/cli/cache.cli.js"
        }
      }
    }
  ]
}
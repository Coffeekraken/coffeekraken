{
    "name": "@coffeekraken/cli",
    "version": "2.0.0-beta.1",
    "description": "Main Coffeekraken toolchain entry point that expose the sugar CLI and let you release the Kraken!",
    "type": "module",
    "bin": {
        "sugar": "dist/pkg/esm/cli/sugar.cli.js"
    },
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "mainSugarEntryPackage": true,
    "dependencies": {
        "@coffeekraken/cli": "^2.0.0-beta.1",
        "@coffeekraken/s-bench": "^2.0.0-beta.1",
        "@coffeekraken/s-event-emitter": "^2.0.0-beta.1",
        "@coffeekraken/s-glob": "^2.0.0-beta.1",
        "@coffeekraken/s-interface": "^2.0.0-beta.1",
        "@coffeekraken/s-log": "^2.0.0-beta.1",
        "@coffeekraken/s-process": "^2.0.0-beta.1",
        "@coffeekraken/s-promise": "^2.0.0-beta.1",
        "@coffeekraken/s-stdio": "^2.0.0-beta.1",
        "@coffeekraken/s-sugar-config": "^2.0.0-beta.1",
        "@coffeekraken/s-sugar-json": "^2.0.0-beta.1",
        "@coffeekraken/sugar": "^2.0.0-beta.1",
        "chalk": "^5.1.2",
        "dotenv": "^16.0.1",
        "enquirer": "^2.4.1",
        "fkill": "^8.1.1",
        "fs-extra": "^10.1.0"
    },
    "main": "dist/pkg/cjs/node/exports.js",
    "module": "dist/pkg/esm/node/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/node/exports.js",
            "import": "./dist/pkg/esm/node/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}

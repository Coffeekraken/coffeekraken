{
    "name": "@coffeekraken/s-class",
    "version": "2.0.0-alpha.20",
    "description": "Base class that can be (and is) used as base class for all your needs. It mainly expose the \"settings\" property which store your passed settings up from your class.",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-event-emitter": "^2.0.0-alpha.20",
        "@coffeekraken/s-interface": "^2.0.0-alpha.20",
        "@coffeekraken/sugar": "^2.0.0-alpha.20"
    },
    "main": "dist/pkg/cjs/shared/exports.js",
    "module": "dist/pkg/esm/shared/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/shared/exports.js",
            "import": "./dist/pkg/esm/shared/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}

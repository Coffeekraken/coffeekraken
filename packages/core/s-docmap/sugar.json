{
  "cli": [
    {
      "stack": "docmap",
      "description": "SDocMap CLI",
      "actions": {
        "build": {
          "description": "Generate the docmap.json file by searching for source files with a @namespace defined",
          "process": "./src/node/build.cli.js"
        },
        "find": {
          "description": "Search for a docmap entry",
          "process": "./src/node/find.cli.js"
        },
        "read": {
          "description": "Get a special docmap entry",
          "process": "./src/node/read.cli.js"
        }
      }
    }
  ]
}

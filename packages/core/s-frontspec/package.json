{
    "name": "@coffeekraken/s-frontspec",
    "version": "2.0.0-alpha.18",
    "description": "A simple class to handle (read) frontspec.json files. A frontspec.json file is a list of things (css, js, styles, etc...) that your frontend project expose and can be used by a backend like wordpress for example...",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-class": "^2.0.0-alpha.18",
        "@coffeekraken/s-env": "^2.0.0-alpha.18",
        "@coffeekraken/s-file": "^2.0.0-alpha.18",
        "@coffeekraken/s-glob": "^2.0.0-alpha.18",
        "@coffeekraken/s-interface": "^2.0.0-alpha.18",
        "@coffeekraken/s-log": "^2.0.0-alpha.18",
        "@coffeekraken/s-process": "^2.0.0-alpha.18",
        "@coffeekraken/s-promise": "^2.0.0-alpha.18",
        "@coffeekraken/s-stdio": "^2.0.0-alpha.18",
        "@coffeekraken/s-sugar-config": "^2.0.0-alpha.18",
        "@coffeekraken/sugar": "^2.0.0-alpha.18",
        "glob": "^7.2.3"
    },
    "main": "dist/pkg/cjs/node/exports.js",
    "module": "dist/pkg/esm/node/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/node/exports.js",
            "import": "./dist/pkg/esm/node/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}

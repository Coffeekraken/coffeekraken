{
    "name": "@coffeekraken/s-package",
    "version": "2.0.0-alpha.13",
    "description": "",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-class": "^2.0.0-alpha.13",
        "@coffeekraken/s-file": "^2.0.0-alpha.13",
        "@coffeekraken/s-glob": "^2.0.0-alpha.13",
        "@coffeekraken/s-interface": "^2.0.0-alpha.13",
        "@coffeekraken/s-log": "^2.0.0-alpha.13",
        "@coffeekraken/s-markdown-builder": "^2.0.0-alpha.13",
        "@coffeekraken/s-promise": "^2.0.0-alpha.13",
        "@coffeekraken/s-sugar-config": "^2.0.0-alpha.13",
        "@coffeekraken/sugar": "^2.0.0-alpha.13",
        "chokidar": "^3.5.3",
        "dependency-cruiser": "^11.14.1",
        "glob": "^8.0.3"
    },
    "main": "dist/pkg/cjs/node/exports.js",
    "module": "dist/pkg/esm/node/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/node/exports.js",
            "import": "./dist/pkg/esm/node/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    }
}

            "@coffeekraken.s-promise.SPromise.js.treatAsValue": {
                "id": "@coffeekraken.s-promise.SPromise.js.treatAsValue",
                "name": "treatAsValue",
                "type": {
                    "types": [
                        {
                            "type": "Function"
                        }
                    ],
                    "raw": "Function"
                },
                "description": "This function allows you to wrap a promise in a ```resolve``` call to prevent\nthis promise to be treated as a \"chaining\" promise but to be treated as\nnormal value passed in the resolve call.",
                "namespace": "@coffeekraken.s-promise.SPromise.js",
                "status": "beta",
                "example": [
                    {
                        "language": "js",
                        "code": "import treatAsValue from '@coffeekraken/sugar/js/promise/treatAsValue';\nawait new Promise(resolve => {\n     const myPromise = new Promise(resolve => {});\n     resolve(treatAsValue(myPromise));\n}); // => myPromise"
                    }
                ],
                "since": "2.0.0",
                "author": {
                    "name": "Olivier Bossel",
                    "email": "olivier.bossel@gmail.com"
                },
                "filename": "treatAsValue.ts",
                "extension": "ts",
                "relPath": "src/shared/treatAsValue.ts",
                "children": {}
            },
            "@coffeekraken.s-promise.shared.interface.SPromiseSettingsInterface": {
                "id": "@coffeekraken.s-promise.shared.interface.SPromiseSettingsInterface",
                "name": "SPromiseSettingsInterface",
                "type": {
                    "types": [
                        {
                            "type": "Class"
                        }
                    ],
                    "raw": "Class"
                },
                "platform": [
                    {
                        "name": "js",
                        "description": ""
                    }
                ],
                "description": "This class represent the interface that describe SPromise settings",
                "namespace": "@coffeekraken.s-promise.shared.interface",
                "status": "beta",
                "interface": {
                    "name": "SPromiseSettingsInterface",
                    "description": "",
                    "definition": {
                        "treatCancelAs": {
                            "description": "Specify if a \"cancel\" event/call has to be treated like a \"resolve\", or like a \"reject\" a promise level",
                            "type": {
                                "types": [
                                    {
                                        "type": "String"
                                    }
                                ],
                                "raw": "String"
                            },
                            "values": [
                                "resolve",
                                "reject"
                            ],
                            "default": "resolve"
                        },
                        "destroyTimeout": {
                            "description": "Specify after how many milliseconds the promise will be destroyed after a \"finally\" event",
                            "type": {
                                "types": [
                                    {
                                        "type": "Number"
                                    }
                                ],
                                "raw": "Number"
                            },
                            "default": 1
                        },
                        "preventRejectOnThrow": {
                            "description": "Specify if you prefer your promise to be \"resolved\" when an underlying error is catched, or if is has to be rejected normally",
                            "type": {
                                "types": [
                                    {
                                        "type": "Boolean"
                                    }
                                ],
                                "raw": "Boolean"
                            },
                            "default": false
                        },
                        "emitLogErrorEventOnThrow": {
                            "description": "Specify if you want a \"log\" of type \"error\" to be emitted when an underlying error is catched",
                            "type": {
                                "types": [
                                    {
                                        "type": "Boolean"
                                    }
                                ],
                                "raw": "Boolean"
                            },
                            "default": true
                        },
                        "resolveAtResolveEvent": {
                            "description": "Specify if youw promise has to be resolved when catching a \"resolve\" event from deeper emitter",
                            "type": {
                                "types": [
                                    {
                                        "type": "Boolean"
                                    }
                                ],
                                "raw": "Boolean"
                            },
                            "default": false
                        },
                        "rejectAtRejectEvent": {
                            "description": "Specify if youw promise has to be rejected when catching a \"reject\" event from deeper emitter",
                            "type": {
                                "types": [
                                    {
                                        "type": "Boolean"
                                    }
                                ],
                                "raw": "Boolean"
                            },
                            "default": false
                        },
                        "resolveProxies": {
                            "description": "Specify some functions to be called just before resolving the promise. This function will take the current promise resolve value and must return the updated value",
                            "type": {
                                "types": [
                                    {
                                        "type": "Array",

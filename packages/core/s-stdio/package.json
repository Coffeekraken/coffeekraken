{
    "name": "@coffeekraken/s-stdio",
    "version": "2.0.0-alpha.6",
    "description": "",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-class": "^2.0.0-alpha.6",
        "@coffeekraken/s-duration": "^2.0.0-alpha.6",
        "@coffeekraken/s-event-emitter": "^2.0.0-alpha.6",
        "@coffeekraken/s-interface": "^2.0.0-alpha.6",
        "@coffeekraken/s-log": "^2.0.0-alpha.6",
        "@coffeekraken/s-promise": "^2.0.0-alpha.6",
        "@coffeekraken/sugar": "^2.0.0-alpha.6",
        "enquirer": "^2.3.6",
    },
    "main": "dist/pkg/cjs/node/exports.js",
    "module": "dist/pkg/esm/node/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/node/exports.js",
            "import": "./dist/pkg/esm/node/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    }
}
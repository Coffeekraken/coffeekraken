{
    "name": "@coffeekraken/s-stdio",
    "version": "2.0.0-alpha.22",
    "description": "Clean and extensible Stdio system that make use of the @coffeekraken/s-event-emitter event system to listen to \"log\" or \"ask\" events and display them nicely with our built-in terminal implementation or anywhere else if you extends it.",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-class": "^2.0.0-alpha.22",
        "@coffeekraken/s-duration": "^2.0.0-alpha.22",
        "@coffeekraken/s-event-emitter": "^2.0.0-alpha.22",
        "@coffeekraken/s-interface": "^2.0.0-alpha.22",
        "@coffeekraken/s-log": "^2.0.0-alpha.22",
        "@coffeekraken/s-promise": "^2.0.0-alpha.22",
        "@coffeekraken/sugar": "^2.0.0-alpha.22",
        "enquirer": "^2.3.6",
        "node-notifier": "^10.0.1"
    },
    "main": "dist/pkg/cjs/js/exports.js",
    "module": "dist/pkg/esm/js/exports.js",
    "exports": {
        "node": {
            "require": "./dist/pkg/cjs/node/exports.js",
            "import": "./dist/pkg/esm/node/exports.js"
        },
        "default": {
            "require": "./dist/pkg/cjs/js/exports.js",
            "import": "./dist/pkg/esm/js/exports.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}
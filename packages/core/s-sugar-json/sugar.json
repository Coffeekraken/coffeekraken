                "search": {
                    "scope": "package",
                    "description": "Search and list for sugar.json files",
                    "interface": "./dist/pkg/esm/node/interface/SSugarJsonSearchParamsInterface.js",
                    "process": "./dist/pkg/esm/cli/search.cli.js"
                }
            }
        }
    ]
}
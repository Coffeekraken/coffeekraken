{
    "config": {
        "folders": [
            {
                "path": "./dist/pkg/%moduleSystem/config"
            }
        ]
    },
    "cli": [
        {
            "stack": "sugarJson",
            "description": "Sugar.json processes like update, etc...",
            "defaultAction": "search",

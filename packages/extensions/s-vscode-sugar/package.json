{
    "name": "s-vscode-sugar",
    "displayName": "s-vscode-sugar",
    "description": "",
    "version": "2.0.0",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:s-vscode-sugar.activate",
        "onCommand:s-vscode-sugar.configurationExplorer"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "s-vscode-sugar.activate",
                "title": "Activate"
            },
            {
                "command": "s-vscode-sugar.configurationExplorer",
                "title": "SConfiguration Explorer"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@coffeekraken/s-request": "^2.0.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "@types/vscode": "^1.58.0",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    },
    "dependencies": {
        "axios": "^0.21.1"
    }
}

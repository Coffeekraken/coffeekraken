{
    "name": "@coffeekraken/s-lazy-feature",
    "version": "2.0.0-alpha.25",
    "description": "This feature allows you to scope your sections, etc... inside a template that will be rendered only when the section comes near the viewport.",
    "type": "module",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-class": "^2.0.0-alpha.25",
        "@coffeekraken/s-env": "^2.0.0-alpha.25",
        "@coffeekraken/s-feature": "^2.0.0-alpha.25",
        "@coffeekraken/s-interface": "^2.0.0-alpha.25",
        "@coffeekraken/sugar": "^2.0.0-alpha.25"
    },
    "main": "dist/pkg/cjs/js/exports.js",
    "module": "dist/pkg/esm/js/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/js/exports.js",
            "import": "./dist/pkg/esm/js/exports.js"
        },
        "./lazy": {
            "require": "./dist/pkg/cjs/js/lazy.js",
            "import": "./dist/pkg/esm/js/lazy.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}

{
    "name": "@coffeekraken/s-sugar-feature",
    "version": "2.0.0-alpha.26",
    "description": "Global feature that you can apply on your body and that will handle convinient things like automatically expand your \"pleasant css\" classes, add/remove a \"scrolled\" class on the body, remove animations when user resize the page, and more...",
    "type": "module",
    "scripts": {
        "dev": "sugar frontstack dev",
        "build": "sugar frontstack build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-class": "^2.0.0-alpha.26",
        "@coffeekraken/s-env": "^2.0.0-alpha.26",
        "@coffeekraken/s-feature": "^2.0.0-alpha.26",
        "@coffeekraken/s-interface": "^2.0.0-alpha.26",
        "@coffeekraken/sugar": "^2.0.0-alpha.26"
    },
    "main": "dist/pkg/cjs/js/exports.js",
    "module": "dist/pkg/esm/js/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/js/exports.js",
            "import": "./dist/pkg/esm/js/exports.js"
        },
        "./lazy": {
            "require": "./dist/pkg/cjs/js/lazy.js",
            "import": "./dist/pkg/esm/js/lazy.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}
{
    "name": "@coffeekraken/s-theme",
    "version": "2.0.0-alpha.27",
    "description": "Central class to handle your theme in the Coffeekraken toolchain. It allows you to switch theme|variant, list available themes, etc...",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@builder.io/partytown": "^0.7.4",
        "@coffeekraken/s-class": "^2.0.0-alpha.27",
        "@coffeekraken/s-classmap": "^2.0.0-alpha.27",
        "@coffeekraken/s-color": "^2.0.0-alpha.27",
        "@coffeekraken/s-env": "^2.0.0-alpha.27",
        "@coffeekraken/s-event-emitter": "^2.0.0-alpha.27",
        "@coffeekraken/s-frontspec": "^2.0.0-alpha.27",
        "@coffeekraken/s-interface": "^2.0.0-alpha.27",
        "@coffeekraken/s-sugar-config": "^2.0.0-alpha.27",
        "@coffeekraken/s-sugar-json": "^2.0.0-alpha.27",
        "@coffeekraken/sugar": "^2.0.0-alpha.27",
        "fastdom": "^1.0.11",
        "known-css-properties": "^0.25.0",
        "micromatch": "^4.0.5"
    },
    "main": "dist/pkg/cjs/js/exports.js",
    "module": "dist/pkg/esm/js/exports.js",
    "exports": {

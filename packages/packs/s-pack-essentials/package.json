{
    "name": "@coffeekraken/s-pack-essentials",
    "version": "2.0.0-beta.1",
    "description": "Expose and init essentials packages like s-activate, s-form-validate, and more...",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-activate-feature": "^2.0.0-beta.1",
        "@coffeekraken/s-appear-feature": "^2.0.0-beta.1",
        "@coffeekraken/s-clipboard-copy-component": "^2.0.0-beta.1",
        "@coffeekraken/s-deps-feature": "^2.0.0-beta.1",
        "@coffeekraken/s-floating-feature": "^2.0.0-beta.1",
        "@coffeekraken/s-form-validate-feature": "^2.0.0-beta.1",
        "@coffeekraken/s-inline-feature": "^2.0.0-beta.1",
        "@coffeekraken/s-lazy-feature": "^2.0.0-beta.1",
        "@coffeekraken/s-refocus-feature": "^2.0.0-beta.1",
        "@coffeekraken/s-scroll-component": "^2.0.0-beta.1",
        "@coffeekraken/s-sugar-feature": "^2.0.0-beta.1"
    },
    "main": "dist/pkg/cjs/js/exports.js",
    "module": "dist/pkg/esm/js/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/js/exports.js",
            "import": "./dist/pkg/esm/js/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}

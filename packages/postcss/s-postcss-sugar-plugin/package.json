{
    "name": "@coffeekraken/s-postcss-sugar-plugin",
    "version": "2.0.0-alpha.24",
    "description": "Postcss Coffeekraken plugin that gives you access to a ton of sugar like \"@sugar.when.dark\", \"@sugar.classes\", etc... mixins as well as functions like \"sugar.padding\", \"sugar.color\", etc... All of these makes uses of your theme configuration defined using the @coffeekraken/s-sugar-config system.",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-bench": "^2.0.0-alpha.24",
        "@coffeekraken/s-color": "^2.0.0-alpha.24",
        "@coffeekraken/s-docblock": "^2.0.0-alpha.24",
        "@coffeekraken/s-duration": "^2.0.0-alpha.24",
        "@coffeekraken/s-env": "^2.0.0-alpha.24",
        "@coffeekraken/s-event-emitter": "^2.0.0-alpha.24",
        "@coffeekraken/s-file": "^2.0.0-alpha.24",
        "@coffeekraken/s-frontspec": "^2.0.0-alpha.24",
        "@coffeekraken/s-glob": "^2.0.0-alpha.24",
        "@coffeekraken/s-interface": "^2.0.0-alpha.24",
        "@coffeekraken/s-process": "^2.0.0-alpha.24",
        "@coffeekraken/s-promise": "^2.0.0-alpha.24",
        "@coffeekraken/s-request": "^2.0.0-alpha.24",
        "@coffeekraken/s-sugar-config": "^2.0.0-alpha.24",
        "@coffeekraken/s-sugar-json": "^2.0.0-alpha.24",
        "@coffeekraken/s-theme": "^2.0.0-alpha.24",
        "@coffeekraken/sugar": "^2.0.0-alpha.24",
        "chokidar": "^3.5.3",
        "csso": "^5.0.5",
        "faker": "^5.5.3",
        "fantasticon": "1.2.3",
        "filehound": "^1.17.6",
        "get-folder-size": "^4.0.0",
        "glob": "^7.2.3",
        "glob-all": "^3.3.1",
        "oslllo-svg-fixer": "^1.4.1",
        "postcss": "^8.4.14",
        "svg-outline-stroke": "^1.3.1"
    },
    "main": "dist/pkg/cjs/node/exports.js",
    "module": "dist/pkg/esm/node/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/node/exports.js",
            "import": "./dist/pkg/esm/node/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}
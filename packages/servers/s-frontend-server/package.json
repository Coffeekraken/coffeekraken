{
    "name": "@coffeekraken/s-frontend-server",
    "version": "2.0.0-alpha.27",
    "description": "Main Coffeekraken frontend server that powers the \"generic\" projects and handle things like folder based routing, views rendering, etc...",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-bench": "^2.0.0-alpha.27",
        "@coffeekraken/s-carpenter": "^2.0.0-alpha.27",
        "@coffeekraken/s-class": "^2.0.0-alpha.27",
        "@coffeekraken/s-docblock": "^2.0.0-alpha.27",
        "@coffeekraken/s-docmap": "^2.0.0-alpha.27",
        "@coffeekraken/s-duration": "^2.0.0-alpha.27",
        "@coffeekraken/s-doc": "^2.0.0-alpha.27",
        "@coffeekraken/s-env": "^2.0.0-alpha.27",
        "@coffeekraken/s-file": "^2.0.0-alpha.27",
        "@coffeekraken/s-frontspec": "^2.0.0-alpha.27",
        "@coffeekraken/s-glob": "^2.0.0-alpha.27",
        "@coffeekraken/s-interface": "^2.0.0-alpha.27",
        "@coffeekraken/s-log": "^2.0.0-alpha.27",
        "@coffeekraken/s-markdown-builder": "^2.0.0-alpha.27",
        "@coffeekraken/s-process": "^2.0.0-alpha.27",
        "@coffeekraken/s-promise": "^2.0.0-alpha.27",
        "@coffeekraken/s-specs": "^2.0.0-alpha.27",
        "@coffeekraken/s-sugar-config": "^2.0.0-alpha.27",
        "@coffeekraken/s-typescript-builder": "^2.0.0-alpha.27",
        "@coffeekraken/s-view-renderer": "^2.0.0-alpha.27",
        "@coffeekraken/sugar": "^2.0.0-alpha.27",
        "body-parser": "^1.20.0",
        "compression": "^1.7.4",
        "express": "^4.18.1",
        "express-fileupload": "^1.4.0",
        "express-remove-route": "^1.0.0",
        "http-proxy-middleware": "^2.0.6",
        "marked": "^4.0.18",
        "minimatch": "^3.1.2",
        "request": "^2.88.2",
        "run-middleware": "^0.9.10"
    },
    "main": "dist/pkg/cjs/node/exports.js",
    "module": "dist/pkg/esm/node/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/node/exports.js",
            "import": "./dist/pkg/esm/node/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}
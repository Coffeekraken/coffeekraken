{
  "config": {
    "folders": [
      {
        "path": "./dist/pkg/%moduleSystem/config"
      }
    ]
  },
  "cli": [
    {
      "stack": "frontendServer",
      "description": "Frontend server API",
      "defaultAction": "start",
      "actions": {
        "start": {
          "description": "Start a new frontend server process",
          "process": "./dist/pkg/esm/cli/start.cli.js"
        },
        "addDefaultPages": {
          "description": "Add default pages to the frontend server like the index, the 404, etc...",
          "process": "./dist/pkg/esm/cli/addDefaultPages.cli.js"
        },
        "corsProxy": {
          "description": "Start a cors proxy server",
          "process": "./dist/pkg/esm/cli/corsProxy.cli.js"
        }
      }
    }
  ]
}
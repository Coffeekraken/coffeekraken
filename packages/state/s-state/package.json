{
    "name": "@coffeekraken/s-state",
    "version": "2.0.0-alpha.17",
    "description": "Simple and convinient state management class that let you subscribe to updates a global or specific level of your state, and more...",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-class": "^2.0.0-alpha.17",
        "@coffeekraken/s-interface": "^2.0.0-alpha.17",
        "@coffeekraken/s-log": "^2.0.0-alpha.17",
        "@coffeekraken/s-promise": "^2.0.0-alpha.17",
        "@coffeekraken/s-sugar-config": "^2.0.0-alpha.17",
        "@coffeekraken/sugar": "^2.0.0-alpha.17"
    },
    "main": "dist/pkg/cjs/shared/exports.js",
    "module": "dist/pkg/esm/shared/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/shared/exports.js",
            "import": "./dist/pkg/esm/shared/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}
{
    "config": {
        "folders": [
            {
                "path": "./dist/pkg/%moduleSystem/config"
            }
        ]
    },
    "cli": [
        {
            "stack": "jest",
            "description": "Jest tester related actions",
            "defaultAction": "start",
            "actions": {
                "start": {
                    "scope": "global",
                    "description": "Start the tests in the current package",
                    "interface": "./dist/pkg/esm/node/interface/SJestTesterStartParamsInterface.js",
                    "process": "./dist/pkg/esm/cli/start.cli.js"
                }
            }
        }
    ]
}
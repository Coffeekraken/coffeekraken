{
    "name": "@coffeekraken/s-frontend-checker",
    "version": "2.0.0-alpha.27",
    "description": "Powerful class that let you specify some (built-in) rules that your DOM has to respect and executing a check with these rules to get what is ok and what's not...",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "scripts": {
        "build": "node --experimental-json-modules --no-warnings --es-module-specifier-resolution node src/node/build.js",
        "deploy": "rsync -avzh --delete --progress --stats ./dist/cdn/ fe0mt_coffeekraken@fe0mt.ftp.infomaniak.com:~/sites/cdnv2.coffeekraken.io/s-frontend-checker"
    },
    "dependencies": {
        "@coffeekraken/s-class": "^2.0.0-alpha.27",
        "@coffeekraken/s-color": "^2.0.0-alpha.27",
        "@coffeekraken/s-duration": "^2.0.0-alpha.27",
        "@coffeekraken/s-file": "^2.0.0-alpha.27",
        "@coffeekraken/s-interface": "^2.0.0-alpha.27",
        "@coffeekraken/s-promise": "^2.0.0-alpha.27",
        "@coffeekraken/sugar": "^2.0.0-alpha.27",
        "aria-query": "^5.2.1",
        "dom-accessibility-api": "^0.6.1",
        "html2canvas": "^1.4.1",
        "puppeteer": "^20.7.3"
    },
    "homepage": "https://coffeekraken.io",
    "main": "dist/pkg/cjs/js/exports.js",
    "module": "dist/pkg/esm/js/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/js/exports.js",
            "import": "./dist/pkg/esm/js/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    }
}

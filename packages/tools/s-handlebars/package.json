{
    "name": "@coffeekraken/s-handlebars",
    "version": "2.0.0-alpha.20",
    "description": "Handlebars utility package that export a \"registerHelpers\" function which will register helpers on your handlebars instance like \"length\", \"replace\", \"sort\", and a lot more...",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-file": "^2.0.0-alpha.20",
        "@coffeekraken/s-promise": "^2.0.0-alpha.20",
        "@coffeekraken/s-sugar-config": "^2.0.0-alpha.20",
        "@coffeekraken/sugar": "^2.0.0-alpha.20"
    },
    "main": "dist/pkg/cjs/node/exports.js",
    "module": "dist/pkg/esm/node/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/node/exports.js",
            "import": "./dist/pkg/esm/node/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}
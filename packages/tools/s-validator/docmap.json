            "@coffeekraken.s-validator.shared.SValidation": {
                "id": "@coffeekraken.s-validator.shared.SValidation",
                "name": "SValidation",
                "type": {
                    "types": [
                        {
                            "type": "Class"
                        }
                    ]
                },
                "description": "This class allows you to validate a lot of data with a lot of validators like \"email\", \"url\", \"min\", \"max\", etc...",
                "namespace": "@coffeekraken.s-validator.shared",
                "status": "beta",
                "example": [
                    {
                        "language": "js",
                        "code": "import __SValidator from '@coffeekraken/s-validation';\nconst validator = new __SValidator();\nvalidator.validate('Hello World Plop', {\n   min: 2,\n   max: 10\n});\n// {\n//   valid: false,\n//   messages: [\n//       'This must have at max 10 characters'\n//   ]\n// }"
                    }
                ],
                "since": "2.0.0",

{
    "name": "@coffeekraken/s-validator",
    "version": "2.0.0-alpha.27",
    "description": "Simple and powerfull class that let you pass a value with rules to validate it, and get back what's ok and what's not with messages for each errors.",
    "type": "module",
    "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
    "license": "MIT",
    "dependencies": {
        "@coffeekraken/s-class": "^2.0.0-alpha.27",
        "@coffeekraken/s-i18n": "^2.0.0-alpha.27",
        "@coffeekraken/s-interface": "^2.0.0-alpha.27",
        "@coffeekraken/sugar": "^2.0.0-alpha.27",
        "@specim3n/types": "1.0.0-alpha.1"
    },
    "main": "dist/pkg/cjs/shared/exports.js",
    "module": "dist/pkg/esm/shared/exports.js",
    "exports": {
        ".": {
            "require": "./dist/pkg/cjs/shared/exports.js",
            "import": "./dist/pkg/esm/shared/exports.js"
        },
        "./shared/*": {
            "require": "./dist/pkg/cjs/shared/*.js",
            "import": "./dist/pkg/esm/shared/*.js"
        },
        "./node/*": {
            "require": "./dist/pkg/cjs/node/*.js",
            "import": "./dist/pkg/esm/node/*.js"
        },
        "./js/*": {
            "require": "./dist/pkg/cjs/js/*.js",
            "import": "./dist/pkg/esm/js/*.js"
        }
    },
    "homepage": "https://coffeekraken.io"
}

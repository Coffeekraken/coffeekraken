                "example": [
                    {
                        "language": "js",
                        "code": "import { rewritePlugin } from '@coffeekraken/s-vite';\n// vite.config.js\n{\n    ...\n    plugins: [\n     rewritePlugin([{\n         match: /something\\.js$/,\n         rewrite(src, id) {\n             return 'hello world';\n         }\n     }])\n   ]\n}"

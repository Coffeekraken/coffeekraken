{
    "type": "Bare",
    "category": "bare",
    "title": "Nude",
    "description": "Render your \"html\" or \"nodes\"",
    "props": {
        "html": {
            "type": "Html",
            "description": "Specify the html to render"
        },
        "nodes": {
            "type": "Html[]",
            "description": "Specify some nodes to render"
        }
    },
    "examples": {
        "twig": {
            "title": "Twig",
            "description": "Here's a twig sample of how to use the card view",
            "code": [
                "{% include 'sugar.bare.container.container' with {",
                "   type: {",
                "       default: '',",
                "       wide: 'full'",
                "   }",
                "   html: '<h2>Hello world</h2>',",
                "   margin: {",
                "       default: {",
                "           bottom: '50'",
                "   },",
                "   mobile: {",
                "       bottom: '20'",
                "   }",
                "} %}"
            ]
        }
    }
}

<a id="{{ id.value }}" class="s-cta s-btn s-color:{{ color.value }} {{ attributes.class }}" {{ __attrs(attributes, ['class'])|raw }}
    href="{{ link.url }}"
    {% if link.newWindow %} target="_blank" {% endif %}
    color="{{ color.value }}"
    title="{{ link.title }}"
>
    {{ link.text }}
</a>    
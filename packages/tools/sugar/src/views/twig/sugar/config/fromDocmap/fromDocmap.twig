<dl>
    {% for key, docmapItem in docmapItems %}
    <dt class="s-font:40 s-mbe:30">
        <header class="s-flex s-bg:main-surface s-radius">
            <div class="s-flex-item:grow s-tc:accent s-p:30 s-typo:strong">
                {{ key|replace('/.*\.config\.[a-zA-Z0-9]+\./', '') }}
            </div>
            <div class="s-typo:bold s-p:30 s-tc:info">{{ docmapItem.type|__typeToString }}</div>
        </header>
        {% if docmapItem.default %}
        <div class="s-pi:30 s-mbs:40">
            <div class="s-typo:code">{{ docmapItem.default }}</div>
        </div>
        {% endif %}
        <p class="s-typo:p s-p:30">{{ docmapItem.description }}</p>
    </dt>
    {% endfor %}
</dl>

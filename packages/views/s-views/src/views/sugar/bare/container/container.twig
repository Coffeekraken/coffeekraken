<div s-container="{{ uid }}" class="{{
    __classes({
        container: type,
        spaces: spaces
    })
    {{ attributes.class }}
    {{ additionalClasses }}
}}" {{ __attrs(attributes, ['class'])|raw }}>
    {{ __nodeDataTemplate('views.bare.container', _context)|raw }}
    {% if nodes %}
        {{ nodes|join()|raw }}
    {% else %}
        {{ html|raw }}
    {% endif %}
</div>
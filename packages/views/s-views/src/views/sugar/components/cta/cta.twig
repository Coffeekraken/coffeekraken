<a id="{{ id }}" class="s-cta s-btn {{ lnf ? 's-btn-' ~ (lnf.value ? lnf.value : lnf) : '' }}
    {{ __classes({
        color: color
    }) }}
    {{ attributes.class }}
    {{ additionalClasses }}
" {{ __attrs(attributes, ['class'])|raw }}
    href="{{ link.url }}"
    {% if link.newWindow %} target="_blank" {% endif %}
    color="{{ color.value }}"
    title="{{ link.title }}"
>
    {{ link.text }}
</a>    
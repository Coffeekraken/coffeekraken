<span class="s-typo:code">
    {{ docmapItem.interface.name}}
</span>

<dl>
    {% for key, property in docmapItem.interface.definition %}
    <dt class="s-font:40 s-mbe:30">
        <header class="s-flex s-bc:main-surface s-radius">
            <div class="s-flex-item:grow s-tc:accent s-p:30 s-typo:strong">
                {{ key }} {% if property.required %} * {% endif %}
            </div>
            <div class="s-typo:bold s-p:30 s-tc:info">{{ this.type }}</div>
        </header>
        {% if property.default %}
        <div class="s-pi:30 s-mbs:40">
            <div class="s-typo:code">{{ property.default }}</div>
        </div>
        {% endif %}
        <p class="s-typo:p s-p:30">{{ property.description|raw }}</p>
    </dt>
    {% endfor %}
</dl>

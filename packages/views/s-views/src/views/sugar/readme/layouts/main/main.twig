{% if 'banner' in sections %}
<!-- image -->
![{{ config.package.json.name }}]({{ config.readme.layout.bannerImageUrl }}?v={{ random() }})
{% endif %}

{% if 'header' in sections %}
    <!-- header -->
    # {{ config.package.json.name }}

    [{{ config.package.json.license }}]({{ config.package.json.repository.url }} "{{ config.package.json.name }} license") {{ config.package.json.version }} - [Git repository]({{ config.package.json.repository.url }} "{{ config.package.json.name }} repository")

    {% if config.readme.shields %}
        <!-- shields -->
        {% for key, shield in config.readme.shields %}
        [![{{ key }}]({{ shield.url|replace({'%packageName':  config.package.json.name}) }})]({{ shield.href|replace({'%packageName': config.package.json.name}) }} "{{ config.package.json.name }} {{ key }}")
        {% endfor %}
    {% endif %}

{% endif %}

{% if 'description' in sections %}
    <!-- description -->
    {{ config.package.json.description }}
{% endif %}

{% if 'install' in sections %}
    <!-- install -->
    ## Install

    ```shell
    npm i {{ config.package.json.name }}
    ```
{% endif %}

<!-- body -->
{% block body %}{% endblock %}

{% if 'license' in sections %}
    <!-- license -->    
    ## License

    Distributed under the **{{config.package.json.license }}** License. See **[LICENSE]({{ config.package.json.repository.url }} "{{ config.package.json.name }} license")** for more information.
{% endif %}

{% if 'contact' in sections %}
    <!-- contact -->
    ## Contact

    Here's all the ways you can contact us listed:

    {% for key, contact in config.contact %}
    [![{{ key }}]({{ contact.shield }})]({{contact.url }} "{{ key }}")
    {% endfor %}
{% endif %}
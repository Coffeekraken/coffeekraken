{
  "exclude": [
    "node_modules",
    "**/node_modules",
    "**/node_modules/**",
    "__tests__",
    "**/__tests__",
    "**/__tests__/**"
  ],
  "compilerOptions": {
    "allowJs": true,
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "noImplicitAny": false,
    "noStrictGenericChecks": false,
    "allowSyntheticDefaultImports": true
  }
}
